@model List<string>
@{
    ViewBag.Title = "GetFile";
}

<h2>GetFile</h2>

<table class="table">
    <tr>
        <th>
            file name
        </th>
        <th>
           Link
        </th>
    </tr>
    @for(var i=0; i <= Model.Count-1; i++)
    {
<tr>
    <td>
        @Model[i].ToString()
    </td>
    <td>
        @Html.ActionLink("Download", "Download", new { filename = @Model[i].ToString()})
    </td>
</tr>
    }
</table>